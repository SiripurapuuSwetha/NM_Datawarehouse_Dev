					"trustServerCertificate": true,
					"authenticationType": "Windows",
					"userName": "[parameters('ConnectToSql_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('ConnectToSql_password')]"
